{
  "expo": {
    "name": "SunAndSkin",
    "slug": "SunAndSkin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/favicon-no-background.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/favicon-no-background.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.madisongrace99.SunAndSkin"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favicon-no-background.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon-no-background.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Pacifico.ttf"
          ]
        }
      ]
    ]
  }
}
